    }
  ],
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.24",
    "10up/wp_mock": "dev-trunk",
    "10up/phpcs-composer": "dev-master"
  },
  "scripts": {
    "lint": "phpcs plugin.php ./includes -s",
    "lint-fix": "phpcbf plugin.php ./includes"
  },
